<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.erppyme.security" default-access="field" default-lazy="false">
	<class name="Rol" table="ROL">
		<id name="codRol" type="integer">
			<column name="CODROL" not-null="true"/>
			<generator class="native"/>
		</id>
		 
		<property name="descripcion" type="string">
			<column name="DESCRIPCION" not-null="true"></column>
		</property>
		
	</class>
</hibernate-mapping> 
