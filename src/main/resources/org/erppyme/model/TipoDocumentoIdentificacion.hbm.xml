<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.erppyme.model" default-access="field" default-lazy="false">
	<class name="TipoDocumentoIdentificacion" table="TIPODOCUMENTOIDENTIFICACION">
		<id name="tipoDocumentoIdentificacionId" type="integer">
			<column name="CODTIPODOCUMENTOIDENTIFICACION" not-null="true"/>
			<generator class="native"/>
		</id>		
		<property name="descripcion" type="string">
			<column name="DESCRIPCION" not-null="true"></column>
		</property>
		
	</class>
</hibernate-mapping> 
