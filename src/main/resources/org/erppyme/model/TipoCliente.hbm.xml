<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.erppyme.model" default-access="field" default-lazy="false">
	<class name="TipoCliente" table="CLIENTE">
		<id name="tipoClienteId" type="integer">
			<column name="CODTIPOCLIENTE" not-null="true"/>
			<generator class="native"/>
		</id>
		
		<property name="nombre" type="string">
			<column name="TIPO" not-null="true"></column>
		</property>
		
	</class>
</hibernate-mapping> 
